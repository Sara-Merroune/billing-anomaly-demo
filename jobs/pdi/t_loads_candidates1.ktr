    <sql>-- A) rows missing in target (deleted or never loaded)
SELECT
  NULLIF(TRIM(br.invoice_id), '') AS invoice_id,
  br.customer_id,
  br.billing_date,
  br.amount,
  br.plan_type,
  br.is_processed,
  br.last_updated_date,
  br.row_checksum,
  SHA2(CONCAT_WS('|',
        br.invoice_id, br.customer_id,
        COALESCE(DATE_FORMAT(br.billing_date,'%Y-%m-%d %H:%i:%s'), ''),
        COALESCE(CAST(br.amount AS CHAR), '')
      ),256) AS row_hash,
  'INSERT' AS strategy
FROM billing_raw br
LEFT JOIN billing_clean bc
  ON bc.invoice_id = br.invoice_id
WHERE bc.invoice_id IS NULL   -- &lt;-- this is the “reinsert deleted” path

UNION ALL

-- B) rows that exist but changed
SELECT
  NULLIF(TRIM(br.invoice_id), '') AS invoice_id,
  br.customer_id,
  br.billing_date,
  br.amount,
  br.plan_type,
  br.is_processed,
  br.last_updated_date,
  br.row_checksum,
  SHA2(CONCAT_WS('|',
        br.invoice_id, br.customer_id,
        COALESCE(DATE_FORMAT(br.billing_date,'%Y-%m-%d %H:%i:%s'), ''),
        COALESCE(CAST(br.amount AS CHAR), '')
      ),256) AS row_hash,
  'UPDATE' AS strategy
FROM billing_raw br
JOIN billing_clean bc
  ON bc.invoice_id = br.invoice_id
WHERE bc.row_hash &lt;&gt; SHA2(CONCAT_WS('|',
        br.invoice_id, br.customer_id,
        COALESCE(DATE_FORMAT(br.billing_date,'%Y-%m-%d %H:%i:%s'), ''),
        COALESCE(CAST(br.amount AS CHAR), '')
      ),256);
</sql>
